{
  "name": "@smy/ui",
  "private": true,
  "version": "0.0.1",
  "author": "kraksin12 <kraksin12@gmail.com> (https://github.com/junhoKims)",
  "repository": {
    "type": "git",
    "url": "https://github.com/junhoKims/smy-info-ui.git"
  },
  "type": "module",
  "files": [
    "build"
  ],
  "main": "./build/index-cjs.js",
  "module": "./build/index-es.js",
  "types": "./build/index.d.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "module": "./build/index-es.js",
      "default": "./build/index-cjs.js"
    },
    "./core": {
      "types": "./build/core/index.d.ts",
      "module": "./build/core/index-es.js",
      "default": "./build/core/index-cjs.js"
    },
    "./core/*": {
      "types": "./build/core/*/index.d.ts",
      "module": "./build/core/*/index-es.js",
      "default": "./build/core/*/index-cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.7.1",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "glob": "^10.3.10",
    "postcss": "^8.4.31",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
